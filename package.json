{
  "name": "City-Info-App",
  "version": "1.0.0",
  "description": "Weather App with various APIs, all glued together by Redux!",
  "main": "index.js",
  "repository": " https://github.com/danielehrlich/City-Info-App.git",
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "dev": "webpack-dev-server --config webpack.dev.config.js",
    "hot": "webpack-dev-server --progress --hot --inline --config webpack.hot.config.js",
    "build": "webpack -p --config webpack.prod.config.js",
    "build -e": "webpack --config webpack.prod.config.js --display-error-details",
    "build2": "webpack --config webpack.prod.config.js && uglify-js ./build/bundle.js -c -m -o ./build/bundle.min.js --source-map ./build/bundle.min.js.map"
  },
  "author": "Daniel",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "eslint": "^3.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.10.1",
    "react-addons-test-utils": "^0.14.7",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "axios": "^0.14.0",
    "babel-preset-stage-1": "^6.1.18",
    "lodash": "^3.10.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-google-maps": "^4.7.1",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.1",
    "react-sparklines": "^1.6.0",
    "redux": "^3.0.4",
    "redux-promise": "^0.5.3"
  }
}
